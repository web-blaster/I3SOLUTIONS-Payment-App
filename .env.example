# ----------------------------------------
# Application
# ----------------------------------------
APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=false
APP_URL=http://localhost

# ----------------------------------------
# Logging
# ----------------------------------------
LOG_CHANNEL=daily
LOG_LEVEL=info

# ----------------------------------------
# Database
# ----------------------------------------
DB_CONNECTION=mysql
DB_HOST=
DB_PORT=3306
DB_DATABASE=
DB_USERNAME=
DB_PASSWORD=

# ----------------------------------------
# Cache / Session
# ----------------------------------------
CACHE_DRIVER=file
SESSION_DRIVER=file
SESSION_LIFETIME=120

# ----------------------------------------
# AWS
# ----------------------------------------
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=ap-south-1
AWS_BUCKET=
FILESYSTEM_DISK=s3

# ----------------------------------------
# Queue (SQS)
# ----------------------------------------
QUEUE_CONNECTION=sqs
SQS_QUEUE=
SQS_PREFIX=

# ----------------------------------------
# External APIs
# ----------------------------------------
EXCHANGE_API_BASE_URL=https://api.exchangerate.host
EXCHANGE_API_TIMEOUT=8

# ----------------------------------------
# Vite / Frontend
# ----------------------------------------
VITE_APP_NAME="${APP_NAME}"

# ----------------------------------------
# JWT
# ----------------------------------------
JWT_SECRET=
JWT_ALGO=HS256
